<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="pa.lab9.cinema.jpa.entities.PersonsEntity">
        <table name="persons" schema="pa_database" catalog=""/>
        <attributes>
            <id name="imdbNameId">
                <column name="imdb_name_id" length="100"/>
            </id>
            <basic name="name">
                <column name="name"/>
            </basic>
            <basic name="birthDetails">
                <column name="birth_details" nullable="false"/>
            </basic>
            <basic name="dateOfBirth">
                <column name="date_of_birth" nullable="false"/>
            </basic>
            <basic name="deathDetails">
                <column name="death_details" nullable="false"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
